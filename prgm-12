import numpy as np

a= np.array([1, 5, 3, 9])

b= np.array([2, 5, 1, 10])

print("a > b:", np.greater(a, b))

print("a >= b:", np.greater_equal(a, b))

print("a < b:", np.less(a, b))

print("a <= b:", np.less_equal(a, b))

print("a == b:", np.equal(a, b))

print("a equal to b within tolerance:", np.allclose(a, b))
